{
  "name": "svelte",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile src/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui-extra/bottom-app-bar": "^7.0.0-beta.0",
    "@smui/checkbox": "^7.0.0-beta.0",
    "@smui/common": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.49.0",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "typescript": "^4.9.5"
  }
}
